Algoritmo Bucle2
	
	Escribir "Indica un numero inicial: "
	Definir N Como Entero
	Leer N
	Escribir "Indica un numero final: "
	Definir M Como Entero
	Leer M
	Escribir "Indica un paso: "
	Definir P Como Entero
	Leer P
	
	Si N<M Entonces
		
		Mientras N<=M Hacer
		
		Escribir N
		N=N+P
		
		FinMientras
	
	Sino
		
		Mientras N>=M Hacer
			
		Escribir N
		N=N+P
			
		FinMientras
		
	FinSi
	
	
FinAlgoritmo
